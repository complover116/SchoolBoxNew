<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/tetridge" />

    <TextView
        android:id="@+id/detdesc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="Results"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/QR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/detdesc"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="68dp"
        android:text="Questions"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/PRC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/QR"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="74dp"
        android:text="Prcs"
        android:textAppearance="?android:attr/textAppearanceLarge" />

	<TextView
	    android:id="@+id/Err1"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@+id/QR"
	    android:layout_centerHorizontal="true"
	    android:layout_marginTop="94dp"
	    android:text="Prcs"
	    android:textAppearance="?android:attr/textAppearanceLarge" />
	
	<TextView
	    android:id="@+id/Err2"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@+id/QR"
	    android:layout_centerHorizontal="true"
	    android:layout_marginTop="114dp"
	    android:text="Prcs"
	    android:textAppearance="?android:attr/textAppearanceLarge" />
	
	<TextView
        android:id="@+id/Err3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/QR"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="134dp"
        android:text="Prcs"
        android:textAppearance="?android:attr/textAppearanceLarge" />
	
	<TextView
        android:id="@+id/Err4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/QR"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="154dp"
        android:text="Prcs"
        android:textAppearance="?android:attr/textAppearanceLarge" />
	
	
	<MediaController
        android:id="@+id/mediaController1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/PRC"
        android:layout_marginRight="25dp"
        android:layout_marginTop="174dp"
        android:layout_toLeftOf="@+id/QR" >
    </MediaController>

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:onClick="GoToMainMenu"
        android:text="Вернуться в главное меню" />

</RelativeLayout>
